<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<template class="FileDialogButton" parent="GtkBox">
		<property name="orientation">horizontal</property>
		<property name="hexpand">False</property>
		<style>
			<class name="linked"/>
		</style>
		<signal name="mnemonic-activate" handler="on_activate" swapped="no"/>
		<child>
			<object class="GtkButton" id="file_btn">
				<signal name="clicked" handler="on_file_btn_clicked" swapped="no"/>
				<child>
					<object class="GtkBox">
						<property name="orientation">horizontal</property>
						<property name="hexpand">True</property>
						<property name="spacing">6</property>
						<child>
							<object class="GtkImage" id="image">
								<property name="icon_name">document-open-symbolic</property>
							</object>
						</child>
						<child>
							<object class="GtkLabel" id="label">
								<property name="label">(None)</property>
							</object>
						</child>
					</object>
				</child>
			</object>
		</child>
		<child>
			<object class="GtkButton" id="clear_btn">
				<property name="visible">False</property>
				<property name="sensitive">False</property>
				<property name="icon_name">user-trash-symbolic</property>
				<signal name="clicked" handler="on_clear_btn_clicked" swapped="no"/>
			</object>
		</child>
		<child>
			<object class="GtkButton" id="reset_btn">
				<property name="visible">False</property>
				<property name="sensitive">False</property>
				<property name="icon_name">edit-undo-symbolic</property>
				<signal name="clicked" handler="on_reset_btn_clicked" swapped="no"/>
			</object>
		</child>
	</template>
</interface>
